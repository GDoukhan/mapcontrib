{
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "classes": true
    },
    "plugins": [
        "import",
        "jest"
    ],
    "extends": "prettier",
    "settings": {
        "import/resolver": "webpack"
    },
    "env": {
        "jest/globals": true
    },
    "globals": {
        "window": true,
        "document": true,
        "MAPCONTRIB": true,
        "$": true,
        "localStorage": true,
        "sessionStorage": true,
        "XMLHttpRequest": true,
        "File": true,
        "FileList": true,
        "FileReader": true,
        "Blob": true,
        "describe": true,
        "it": true,
        "L": true,
        "before": true,
    }
}
