<section id="contrib_form_column" class="column right red">
    <header>
        <button type="button" class="btn btn-link btn-lg pull-right close_btn">
            <i class="fa fa-close"></i>
        </button>

        <h2 data-l10n-id="contribColumn_title"></h2>
    </header>

    <div class="container-fluid content">
        <div class="row">
            <div class="col-xs-12">
                <form>
                    <div class="rg_tag_list"></div>

                    <div class="sticky-footer">
                        <button type="button" class="btn btn-default add_btn" data-l10n-id="contribColumn_addTag"></button>
                        <button type="submit" class="btn btn-primary pull-right place_btn" data-l10n-id="contribFormColumn_placeOnMap"></button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>
